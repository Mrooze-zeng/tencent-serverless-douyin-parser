{
  "name": "douyin-parser",
  "version": "1.0.0",
  "description": "",
  "main": "sls.js",
  "scripts": {
    "start": "PORT=9000 NODE_ENV=prod node app.mjs",
    "dev": "PORT=9999 NODE_ENV=dev nodemon app.mjs ",
    "run-client": "cd client && PORT=3006 yarn start",
    "run-huihui": "cd huihui && PORT=3007 yarn start",
    "build-huihui": "rm -rf statics/** && cd client && npm run build",
    "build-client": "rm -rf huihui-statics/** && cd huihui && npm run build",
    "deploy": "npm run build-client && npm run build-huihui && sls deploy"
  },
  "author": "Mrooze",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "express": "^4.17.1",
    "http-proxy-middleware": "^2.0.6",
    "lowdb": "^3.0.0",
    "node-fetch": "^3.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/node": "^7.17.10",
    "nodemon": "^2.0.16"
  }
}
